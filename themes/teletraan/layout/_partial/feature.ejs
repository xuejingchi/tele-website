<section id="feature" class="feature feature-section">
    <div class="container">
        <% for (var i in theme.Homepage.feature.pic){ %>
            <div class="content">
                <embed src="<%= theme.Homepage.feature.pic[i] %>" />
                <br/>
                <span class="title"><%= __('feature.'+theme.Homepage.feature.title[i]) %></span>
                <br/>
                <span class="detail"><%= __('feature.'+theme.Homepage.feature.name[i]) %></span>
            </div>
        <% } %>
    </div>
    <div style="background: #f5f5f5;">
        <div class="container" style="padding-top: 120px;padding-bottom: 120px;margin-top: 120px;">
            <div class="pic-area" style="padding:0;">
                <div class="col-xs-12">
                    <img class="col-xs-6 img-left" src="<%= theme.Homepage.feature.area[0] %>" alt="teletraan">
                    <div class="col-xs-6 content-left">
                        <div class="pic-title">xxxx</div>
                        <div class="pic-detail">xxxx</div>
                    </div>
                </div>
                <div class="col-xs-12 pic-content">
                    <img class="col-xs-6 img-right" src="<%= theme.Homepage.feature.area[1] %>" alt="teletraan">
                    <div class="col-xs-6 content-right">
                        <div class="pic-title">xxxx</div>
                        <div class="pic-detail">xxxx</div>
                    </div>
                </div>
                <div class="col-xs-12 pic-content">
                    <img class="col-xs-6 img-left" src="<%= theme.Homepage.feature.area[1] %>" alt="teletraan">
                    <div class="col-xs-6 content-left">
                        <div class="pic-title">xxxx</div>
                        <div class="pic-detail">xxxx</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>